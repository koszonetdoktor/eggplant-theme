{
  "name": "eggplant",
  "displayName": "Eggplant Theme",
  "description": "Purple based color theme",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Eggplant light",
        "uiTheme": "vs",
        "path": "./themes/Eggplant-color-theme.json"
      },
      {
        "label": "Eggplant dark",
        "uiTheme": "vs",
        "path": "./themes/Eggplant-dark-color-theme.json"
      }
    ]
  },
  "scripts": {
    "release": "standard-version"
  },
  "dependencies": {
    "vsce": "^2.10.2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "improvement",
        "section": "Improvements"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ]
  }
}
